{
  "name": "logan-jasin-website-cdk",
  "version": "1.0.0",
  "bin": {
    "logan_jasin_personal_website_cdk": "bin/app.js"
  },
  "scripts": {
    "clean": "rm -rf cdk.out dist node_modules",
    "build": "npm install && tsc",
    "format": "prettier --write \"**/*.ts\"",
    "release": "npm run clean && npm run format && npm run build && cdk list",
    "watch": "tsc -w",
    "cdk": "cdk"
  },
  "devDependencies": {
    "@types/node": "22.7.9",
    "aws-cdk": "2.1016.1",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "~5.6.3"
  },
  "dependencies": {
    "aws-cdk-lib": "2.196.0",
    "constructs": "^10.0.0"
  }
}
